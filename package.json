{
  "name": "aisupport",
  "displayName": "AiSupport",
  "description": "Ai augmented softwar development",
  "version": "0.0.1",
  "repository": {
    "url": "https://github.com/Nocreeds/aisupport"
  },
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView.aisupport.chatView"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.startAi",
        "title": "startAi"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "aisupport",
          "title": "AiSupport",
          "icon": "resources/icon.png"
        }
      ]
    },
    "views": {
      "aisupport": [
				{
					"type": "webview",
					"id": "aisupport.chatView",
					"name": "AiSupport"
				}
      ]
    },
    "configuration": {
      "title": "AiSupport",
      "type": "object",
      "properties": {
        "aisupport.apikey": {
          "type": "string",
          "description": "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",
          "order": 1
        },
        "aisupport.organization": {
          "type": "string",
          "description": "org-aWuJHW5k7Nfj1YHKDpuQLIu0",
          "order": 2
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.82.0",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "axios": "^1.5.1",
    "chatgpt": "^5.2.5",
    "openai": "^4.11.1"
  }
}
